{
  "name": "fighting-game-retention-system",
  "version": "1.0.0",
  "description": "Modern PlayCanvas fighting game with retention system",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch & serve -s . -p 5000",
    "build": "tsc && rollup -c",
    "build:client": "cd src/client && bun run build",
    "build:backend": "cd src/backend && bun run build",
    "clean": "rm -rf dist && tsc --build --clean",
    "test": "bun test",
    "lint": "eslint src/ types/ --ext .ts,.tsx",
    "lint:fix": "eslint src/ types/ --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "serve": "serve -s . -p 5000",
    "start": "serve -s . -p 5000"
  },
  "keywords": [
    "playcanvas",
    "fighting-game",
    "retention",
    "ethical-monetization",
    "player-respecting",
    "typescript",
    "webgl"
  ],
  "author": "Fighting Game Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.11",
    "@types/web": "^0.0.127",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "rollup": "^4.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "serve": "^14.2.1",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "playcanvas": "^1.69.0"
  },
  "peerDependencies": {
    "playcanvas": "^1.0.0"
  }
}