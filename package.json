
{
  "name": "sf3-3rd-strike-playcanvas",
  "version": "2.0.0",
  "description": "Street Fighter III: 3rd Strike - PlayCanvas Edition",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch & node dist/index.js",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "fix": "npx ts-node tools/fix_compilation_errors.ts",
    "convert": "npx ts-node tools/complete_typescript_conversion.ts",
    "serve": "http-server . -p 5000 -c-1"
  },
  "dependencies": {
    "playcanvas": "^1.68.3",
    "@playcanvas/web-components": "^1.0.0",
    "express": "^4.18.2",
    "http-server": "^14.1.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "street-fighter",
    "fighting-game",
    "playcanvas",
    "typescript",
    "game-development"
