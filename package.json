{
  "name": "sf3-3rd-strike-playcanvas",
  "version": "2.0.0",
  "description": "Street Fighter III: 3rd Strike - PlayCanvas Edition",
  "main": "src/index.ts",
  "scripts": {
    "build:client": "esbuild src/index.ts --bundle --platform=browser --format=iife --global-name=SF3App --outfile=public/bundle.js --sourcemap --target=es2020 --alias:playcanvas/*=playcanvas/* --alias:playcanvas=src/vendor/pc.cts --external:fflate",
    "build:client:prod": "esbuild src/index.ts --bundle --platform=browser --format=iife --global-name=SF3App --outfile=public/bundle.js --target=es2020 --alias:playcanvas/*=playcanvas/* --alias:playcanvas=src/vendor/pc.cts --external:fflate",
    "build": "node -e \"require('ts-node/register');require('./tools/import_sf3_char_data.ts')\" && npm run build:client",
    "build:prod": "node -e \"require('ts-node/register');require('./tools/import_sf3_char_data.ts')\" && npm run build:client:prod",
    "postbuild": "mkdir -p public/data public/assets && node -e \"require('ts-node/register');require('./tools/encrypt_assets.ts')\" && node -e \"require('ts-node/register');require('./tools/generate_manifest.ts')\"",
    "dev": "npm run build:client -- --watch",
    "start": "npm run build && npm run serve",
    "preview": "node -e \"require('ts-node/register');require('./tools/dev_server.ts')\"",
    "import:sf3": "node ./tools/import_sf3_char_data.js && npm run build",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "fix": "npx ts-node tools/fix_compilation_errors.ts",
    "convert": "npx ts-node tools/complete_typescript_conversion.ts",
    "serve": "http-server public -p 5000 -c 604800 --cors --gzip --brotli"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "@vercel/node": "^5.3.16",
    "bcrypt": "^6.0.0",
    "express": "^4.18.2",
    "fastify": "^5.5.0",
    "http-server": "^14.1.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "playcanvas": "^1.65.3",
    "uuid": "^11.1.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "esbuild": "^0.25.1",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "puppeteer": "^24.21.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "street-fighter",
    "fighting-game",
    "playcanvas",
    "typescript",
    "game-development"
  ],
  "author": "SF3 Development Team",
  "license": "MIT",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "packageManager": "npm@10.9.2"
}
